<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %>
  <td><%= link_to t('common.show'), admin_test_path(test) %></td>
  <td><%= link_to t('common.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('common.delete'),
      admin_test_path(test),
      method: :delete,
      data: { confirm: 'Are you sure?' } %>
  </td>
</tr>
